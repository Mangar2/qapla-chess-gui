{
    "C_Cpp.default.intelliSenseMode": "macos-clang-arm64",
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "C_Cpp.errorSquiggles": "enabled",
    "C_Cpp.codeAnalysis.clangTidy.enabled": true,
    "C_Cpp.codeAnalysis.clangTidy.args": [
        "--std=c++20"
    ],
    "cmake.configureOnOpen": true,
    "cmake.generator": "Ninja",
    "C_Cpp.default.compilerPath": "/opt/homebrew/opt/llvm/bin/clang++",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/**",
        "/opt/homebrew/Cellar/llvm/20.1.8/include/c++/v1",
        "/opt/homebrew/Cellar/llvm/20.1.8/lib/clang/20/include",
        "/Library/Developer/CommandLineTools/SDKs/MacOSX15.sdk/usr/include",
        "/Library/Developer/CommandLineTools/SDKs/MacOSX15.sdk/System/Library/Frameworks"   
    ],
    "C_Cpp.default.defines": [
        "MACOS",
        "ARM"
    ],
    "files.associations": {
        "__bit_reference": "cpp",
        "__hash_table": "cpp",
        "__locale": "cpp",
        "__node_handle": "cpp",
